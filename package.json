{
  "name": "wetvlo",
  "version": "0.0.14",
  "description": "CLI application for monitoring and downloading TV series episodes from Chinese video sites (wetv.vip, iq.com)",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": "./dist/index.js",
  "exports": "./dist/index.js",
  "scripts": {
    "start": "bun run src/index.ts",
    "start:once": "bun run src/index.ts --once",
    "build": "tsup",
    "lint": "biome check . && dprint check",
    "format": "biome check --write --unsafe . && dprint fmt",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "^1.3.6",
    "@types/js-yaml": "^4.0.9",
    "dprint": "^0.51.1",
    "lefthook": "^2.0.15",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "video",
    "downloader",
    "yt-dlp",
    "wetv",
    "iqiyi",
    "chinese",
    "tv-series",
    "cli"
  ],
  "author": "Maxim (neiromaster) Gavrilenko",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neiromaster/wetvlo.git"
  },
  "bugs": {
    "url": "https://github.com/neiromaster/wetvlo/issues"
  },
  "homepage": "https://github.com/neiromaster/wetvlo#readme",
  "dependencies": {
    "cheerio": "^1.1.2",
    "cmd-ts": "^0.14.3",
    "cron-parser": "^4.9.0",
    "execa": "^9.6.1",
    "js-yaml": "^4.1.1",
    "playwright": "^1.58.1",
    "zod": "^4.3.6"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
